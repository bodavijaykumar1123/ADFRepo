{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "TestDatafactory04"
		},
		"ds_sqltable_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ds_sqltable'"
		},
		"LS_ADLSGen2Connection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azuredatalakegen2demo002.dfs.core.windows.net"
		},
		"LS_BLOBConnection_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "BlobConnectionString"
		},
		"LS_HTTPConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseURL}"
		},
		"LS_Keyvalut_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://adfkeyvaulttest01.vault.azure.net/"
		},
		"EventTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/0b09d9a2-10b0-4bdb-966c-fa3ebfa3ba0a/resourceGroups/TestRG001/providers/Microsoft.Storage/storageAccounts/teststorageaccountsa001"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "TestDatafactory04_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ds_sqltable_connectionString": {
						"value": "[parameters('ds_sqltable_connectionString')]"
					},
					"LS_HTTPConnection_properties_typeProperties_url": {
						"value": "[parameters('LS_HTTPConnection_properties_typeProperties_url')]"
					},
					"LS_Keyvalut_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_Keyvalut_properties_typeProperties_baseUrl')]"
					},
					"LS_ADLSGen2Connection_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSGen2Connection_properties_typeProperties_url')]"
					},
					"LS_BLOBConnection_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_BLOBConnection_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "TestDatafactory04_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"EventTrigger_properties_typeProperties_scope": {
						"value": "[parameters('EventTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'TestDatafactory04_ArmTemplate_0')]"
			]
		}
	]
}